<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  	<servlet>
		<servlet-name>CryptoServlet</servlet-name>
		<servlet-class>com.xm.crypto.recommendation.servlet.CryptoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CryptoSortedServlet</servlet-name>
		<servlet-class>com.xm.crypto.recommendation.servlet.CryptoSortedServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CryptoDateServlet</servlet-name>
		<servlet-class>com.xm.crypto.recommendation.servlet.CryptoDateServlet</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>CryptoServlet</servlet-name>
		<url-pattern>/crypto/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CryptoSortedServlet</servlet-name>
		<url-pattern>/sorted/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CryptoDateServlet</servlet-name>
		<url-pattern>/date/*</url-pattern>
	</servlet-mapping>
  
  
</web-app>
